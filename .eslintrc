{
  "parser": "babel-eslint",
  "plugins": ["react", "prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "rules": {
    "no-undef": "off",
    "no-console": "off",
    "react/prop-types": "off",
    "react/no-find-dom-node": "off"
  }
}
